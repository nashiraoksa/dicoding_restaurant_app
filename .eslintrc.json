{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": "google",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "linebreak-style": ["error", "windows"],
    "space-before-function-paren": ["error", "always"],
    "max-len": 0,
    "object-curly-spacing": ["always"]
  }
}
